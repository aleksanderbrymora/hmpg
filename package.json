		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.12",
		"ts-node": "^10.7.0",
		"tslib": "^2.3.1",
		"typescript": "^4.6.2"
	},
	"type": "module",
	"dependencies": {
		"@felte/validator-zod": "^1.0.2",
		"@prisma/client": "3.10.0",
		"bcryptjs": "^2.4.3",
		"cookie": "^0.4.2",
		"prisma": "^3.10.0",
		"sass": "^1.49.9",
		"ts-pattern": "^3.3.5",
		"zod": "^3.12.1"
	}
